[Unit]
Description=RyuBot Service 
After=network.target
After=systemd-user-sessions.service
After=network-online.target

[Service]
Type=forking
ExecStart=/home/ubuntu/RyuBot/ubuntu.16.04-x64/Start.sh
ExecStart = / home / ubuntu / RyuBot / ubuntu.16.04-x64 / Start.sh
ExecStop = / home / ubuntu / RyuBot / ubuntu.16.04-x64 / Start.sh
ExecReload = / home / ubuntu / RyuBot / ubuntu.16.04-x64 / Start.sh

[Install]
WantedBy=multi-user.target